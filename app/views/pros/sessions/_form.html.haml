%h2 Log in
= simple_form_for(Pro.new, as: :pro, url: session_path(:pro)) do |f|
  .form-inputs
    = f.input :email, required: false, autofocus: true
    = f.input :password, required: false
    -# .checkboxes
    = f.input :remember_me, as: :boolean, wrapper: :inline_checkbox if devise_mapping.rememberable?
  .form-actions
    = f.button :submit, "Log in"
= render "pros/shared/links", resource_name: devise_mapping_for('Pro').name, devise_mapping: devise_mapping_for('Pro'), resource_class: devise_mapping_for('Pro').to
